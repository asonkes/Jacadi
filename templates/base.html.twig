		<meta name="twitter:title" content="Jacadi">
		<meta name="twitter:description" content="Jacadi référence de la mode enfantine afin de répondre aux besoins des tous petits">
		<meta name="twitter:image" content="{{ absolute_url(asset('images/twitterImages/doudou2.webp')) }}">

		<title>
			{% block title %}Jacadi
			{% endblock %}
		</title>

		<script src="https://kit.fontawesome.com/2cbf82340f.js" crossorigin="anonymous"></script>

		<link rel="icon" href="{{ asset('images/icons/jacadi_icone.ico') }}" type="image/x-icon">
		{% block stylesheets %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}

		{{ encore_entry_link_tags('app') }}
	</head>

	<body>
		{% include "partials/header.html.twig" %}

		{% block body %}{% endblock %}

		<script>
			<a href="https://www.iubenda.com/privacy-policy/18385874" class="iubenda-white iubenda-noiframe iubenda-embed iubenda-noiframe " title="Politique de confidentialité ">Politique de confidentialité</a>
			<script type="text/javascript">
				(function (w, d) {
var loader = function () {
var s = d.createElement("script"),
tag = d.getElementsByTagName("script")[0];
s.src = "https://cdn.iubenda.com/iubenda.js";
tag.parentNode.insertBefore(s, tag);
};
if (w.addEventListener) {
w.addEventListener("load", loader, false);
} else if (w.attachEvent) {
w.attachEvent("onload", loader);
} else {
w.onload = loader;
}
})(window, document);
			</script>

			{% include "partials/footer.html.twig" %}

			<div id="App"></div>

			{% block javascripts %}
				{{ encore_entry_script_tags('app') }}
			{% endblock %}
		</body>
